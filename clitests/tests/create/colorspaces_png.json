{
    "description": "Test PNG input images with various color spaces.",
    "command": "ktx create --testrun --format ${format} input/png/colorspace_${subcase}.png output/create/colorspaces_png/colorspace_${format}_${subcase}.ktx2",
    "status": 0,
    "outputs": {
        "output/create/colorspaces_png/colorspace_${format}_${subcase}.ktx2": "golden/create/colorspaces_png/colorspace_${format}_${subcase}.ktx2"
    },
    "args": {
        "format": [
            "R8G8B8A8_UNORM",
            "R8G8B8A8_SRGB"
        ],
        "subcase": [
            "default",
            "sRGB",
            "sRGB_gamma_2_2",
            "gamma_1_8",
            "gamma_2_2",
            "gamma_2_4",
            "gamma_1_0",
            "cHRM_BT709",
            "cHRM_BT601_EBU",
            "cHRM_BT601_SMPTE",
            "cHRM_BT2020",
            "cHRM_CIEXYZ",
            "cHRM_ACES",
            "cHRM_ACEScc",
            "cHRM_NTSC1953",
            "cHRM_PAL525",
            "cHRM_DisplayP3",
            "cHRM_AdobeRGB",
            "cHRM_unknown",
            "sRGB_cHRM",
            "gamma_cHRM",
            "sRGB_gamma_cHRM"
        ]
    }
}
