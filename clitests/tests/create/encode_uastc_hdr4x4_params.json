{
    "description": "Test create with UASTC HDR encoding with various parameter combinations.",
    "command": "ktx create --testrun --raw --format ${subcase} --width 8 --height 8 --threads 1 --encode uastc-hdr-4x4 ${select[args]} input/raw/raw_${subcase}_2D_8x8.raw output/create/encode_uastc_hdr4x4_params/output_${subcase}_${select[id]}.ktx2",
    "status": 0,
    "outputTolerance": 0.08,
    "outputs": {
        "output/create/encode_uastc_hdr4x4_params/output_${subcase}_${select[id]}.ktx2": "golden/create/encode_uastc_hdr4x4_params/output_${subcase}_${select[id]}.ktx2"
    },
    "args": {
        "select": [
            { "id": "quality_0", "args": "--uastc-quality 0 --zlib 8" },
            { "id": "quality_4", "args": "--uastc-quality 4 --zstd 3" },
            { "id": "uber", "args": "--uastc-hdr-uber-mode" },
            { "id": "ultra_quant", "args": "--uastc-hdr-ultra-quant" },
            { "id": "favor_astc", "args": "--uastc-hdr-favor-astc" },
            { "id": "combination_1", "args": "--uastc-quality 2 --uastc-rdo --uastc-rdo-m --uastc-rdo-l 4.25 --uastc-rdo-b 1.25 --no-sse --zlib 5" },
            { "id": "combination_2", "args": "--uastc-quality 1 --uastc-rdo --uastc-rdo-m --uastc-rdo-d 64 --normal-mode --zstd 1" },
            { "id": "combination_3", "args": "--uastc-rdo --uastc-rdo-m --uastc-rdo-d 200 --uastc-rdo-f --zstd 22 --normal-mode" },
            { "id": "combination_4", "args": "--uastc-rdo --uastc-rdo-m --uastc-rdo-s 5000.0 --uastc-rdo-l 9.9  --uastc-rdo-b 1.0 --zstd 8" },
            { "id": "combination_5", "args": "--uastc-rdo --uastc-rdo-m --uastc-quality 0 --uastc-rdo-f --uastc-rdo-d 3000 --zlib 8 --uastc-rdo-s 0.01" }
        ],
        "subcase": [
            "R16G16B16_SFLOAT",
            "R16G16B16A16_SFLOAT"
        ]
    }
}
