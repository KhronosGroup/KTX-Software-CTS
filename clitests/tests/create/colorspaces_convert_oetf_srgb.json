{
    "description": "Test --convert-oetf parameter set to srgb.",
    "command": "ktx create --testrun --format ${format} --convert-oetf srgb input/png/colorspace_${subcase}.png output/create/colorspaces_convert_oetf_srgb/output_${format}_${subcase}.ktx2",
    "status": 0,
    "outputs": {
        "output/create/colorspaces_convert_oetf_srgb/output_${format}_${subcase}.ktx2": "golden/create/colorspaces_convert_oetf_srgb/output_${format}_${subcase}.ktx2"
    },
    "args": {
        "format": [
            "R8G8B8A8_UNORM",
            "R8G8B8A8_SRGB"
        ],
        "subcase": [
            "default",
            "sRGB",
            "sRGB_gamma_2_2",
            "gamma_1_8",
            "gamma_2_2",
            "gamma_2_4",
            "gamma_1_0"
        ]
    }
}
