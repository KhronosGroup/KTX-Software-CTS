{
    "description": "Test conversion of various unicode-named files with explicitly named unicode output file.",
    "command": "ktx convert --testrun --input-type ${subcase[type]} input/${subcase[type]}/${subcase[name]}.${subcase[type]} output/convert/explicit/from_${subcase[type]}/${subcase[name]}.ktx2",
    "status": 0,
    "outputs": {
        "output/convert/explicit/from_${subcase[type]}/${subcase[name]}.ktx2": "golden/convert/ktx2/output_${subcase[name]}.ktx2"
    },
    "args": {
        "subcase": [
            { "type": "ktx", "name": "نَسِيج" },
            { "type": "ktx", "name": "テクスチャ" },
            { "type": "ktx", "name": "质地" },
            { "type": "ktx", "name": "조직" },
            { "type": "ktx", "name": "hűtő" }
        ]
    }
}
