{
    "description": "KTXcubemapIncomplete should not set every bit. Normal cube map could be used instead.",
    "command": "ktx validate ${warnAsErr[flag]} ${format[args]} input/validate/7102/error_KTXcubemapIncompleteAllBitsSet_${subcase}.ktx2",
    "status": "${warnAsErr[status]}",
    "stdout": "golden/validate/7102/${warnAsErr[outPrefix]}_KTXcubemapIncompleteAllBitsSet_${subcase}.out.${format[ext]}",
    "args": {
        "warnAsErr": [
            {
                "flag": "",
                "status": 0,
                "outPrefix": "warning"
            },
            {
                "flag": "--warnings-as-errors",
                "status": 3,
                "outPrefix": "error"
            }
        ],
        "format": [
            {
                "args": "",
                "ext": "txt"
            },
            {
                "args": "--format text",
                "ext": "txt"
            },
            {
                "args": "--format json",
                "ext": "json"
            },
            {
                "args": "--format mini-json",
                "ext": "mini.json"
            }
        ],
        "subcase": [
            "00111111"
        ]
    }
}
