{
    "description": "Test extraction from 2D Array textures to raw output files with --all to export every image.",
    "command": "ktx extract --testrun --raw --all input/ktx2/valid_${subcase}.ktx2 output/extract/raw_all_2d_array/output_${subcase}",
    "status": 0,
    "outputs": {
        "output/extract/raw_all_2d_array/output_${subcase}/output_layer0.raw": "golden/extract/raw_all_2d_array/output_${subcase}/output_layer0.raw",
        "output/extract/raw_all_2d_array/output_${subcase}/output_layer1.raw": "golden/extract/raw_all_2d_array/output_${subcase}/output_layer1.raw",
        "output/extract/raw_all_2d_array/output_${subcase}/output_layer2.raw": "golden/extract/raw_all_2d_array/output_${subcase}/output_layer2.raw",
        "output/extract/raw_all_2d_array/output_${subcase}/output_layer3.raw": "golden/extract/raw_all_2d_array/output_${subcase}/output_layer3.raw"
    },
    "args": {
        "subcase": [
            "A1R5G5B5_UNORM_PACK16_2D_ARRAY",
            "A2R10G10B10_SINT_PACK32_2D_ARRAY",
            "A2R10G10B10_UINT_PACK32_2D_ARRAY",
            "A4R4G4B4_UNORM_PACK16_2D_ARRAY",
            "R16G16B16A16_UNORM_2D_ARRAY",
            "R16G16B16_UNORM_2D_ARRAY",
            "R16G16_UNORM_2D_ARRAY",
            "R16_UNORM_2D_ARRAY",
            "R32G32B32A32_UINT_2D_ARRAY",
            "R32G32B32_UINT_2D_ARRAY",
            "R32G32_UINT_2D_ARRAY",
            "R32_UINT_2D_ARRAY",
            "R4G4B4A4_UNORM_PACK16_2D_ARRAY",
            "R4G4_UNORM_PACK8_2D_ARRAY",
            "R5G5B5A1_UNORM_PACK16_2D_ARRAY",
            "R5G6B5_UNORM_PACK16_2D_ARRAY",
            "R8G8B8A8_UNORM_2D_ARRAY",
            "R8G8B8_UNORM_2D_ARRAY",
            "R8G8_UNORM_2D_ARRAY",
            "R8_SINT_2D_ARRAY",
            "R8_SRGB_2D_ARRAY",
            "R8_UINT_2D_ARRAY",
            "R8_UNORM_2D_ARRAY"
        ]
    }
