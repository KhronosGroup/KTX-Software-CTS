{
    "description": "Test extraction from 3D Mip textures to raw output files with --all to export every image.",
    "command": "ktx extract --testrun --raw --all input/ktx2/valid_${subcase}.ktx2 output/extract/raw_all_3d/output_${subcase}",
    "status": 0,
    "outputs": {
        "output/extract/raw_all_3d/output_${subcase}/output_depth0.raw": "golden/extract/raw_all_3d/output_${subcase}/output_depth0.raw",
        "output/extract/raw_all_3d/output_${subcase}/output_depth1.raw": "golden/extract/raw_all_3d/output_${subcase}/output_depth1.raw",
        "output/extract/raw_all_3d/output_${subcase}/output_depth2.raw": "golden/extract/raw_all_3d/output_${subcase}/output_depth2.raw",
        "output/extract/raw_all_3d/output_${subcase}/output_depth3.raw": "golden/extract/raw_all_3d/output_${subcase}/output_depth3.raw"
    },
    "args": {
        "subcase": [
            "A1R5G5B5_UNORM_PACK16_3D",
            "A2R10G10B10_SINT_PACK32_3D",
            "A2R10G10B10_UINT_PACK32_3D",
            "A4R4G4B4_UNORM_PACK16_3D",
            "R16G16B16A16_UNORM_3D",
            "R16G16B16_UNORM_3D",
            "R16G16_UNORM_3D",
            "R16_UNORM_3D",
            "R32G32B32A32_UINT_3D",
            "R32G32B32_UINT_3D",
            "R32G32_UINT_3D",
            "R32_UINT_3D",
            "R4G4B4A4_UNORM_PACK16_3D",
            "R4G4_UNORM_PACK8_3D",
            "R5G5B5A1_UNORM_PACK16_3D",
            "R5G6B5_UNORM_PACK16_3D",
            "R8G8B8A8_UNORM_3D",
            "R8G8B8_UNORM_3D",
            "R8G8_UNORM_3D",
            "R8_SINT_3D",
            "R8_SRGB_3D",
            "R8_UINT_3D",
            "R8_UNORM_3D"
        ]
    }
}
