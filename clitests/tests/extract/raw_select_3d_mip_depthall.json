{
    "description": "Test extraction from 3D Mip textures to combined (including every depth slice) raw \"3D\" output files by not specifying the --depth or --all.",
    "command": "ktx extract --testrun --raw ${select[flags]} input/ktx2/valid_${subcase}.ktx2 output/extract/raw_select_3d_mip_depthall/output_${subcase}/output${select[id]}.raw",
    "status": 0,
    "outputs": {
        "output/extract/raw_select_3d_mip_depthall/output_${subcase}/output${select[id]}.raw": "golden/extract/raw_select_3d_mip_depthall/output_${subcase}/output${select[id]}.raw"
    },
    "args": {
        "select": [
            { "id": "_level0_depth_all", "flags": "--level 0" },
            { "id": "_level1_depth_all", "flags": "--level 1" },
            { "id": "_level2_depth_all", "flags": "--level 2" },
            { "id": "_level3_depth_all", "flags": "--level 3" }
        ],
        "subcase": [
            "A1R5G5B5_UNORM_PACK16_3D_MIP",
            "A2R10G10B10_SINT_PACK32_3D_MIP",
            "A2R10G10B10_UINT_PACK32_3D_MIP",
            "A4R4G4B4_UNORM_PACK16_3D_MIP",
            "R16G16B16A16_UNORM_3D_MIP",
            "R16G16B16_UNORM_3D_MIP",
            "R16G16_UNORM_3D_MIP",
            "R16_UNORM_3D_MIP",
            "R32G32B32A32_UINT_3D_MIP",
            "R32G32B32_UINT_3D_MIP",
            "R32G32_UINT_3D_MIP",
            "R32_UINT_3D_MIP",
            "R4G4B4A4_UNORM_PACK16_3D_MIP",
            "R4G4_UNORM_PACK8_3D_MIP",
            "R5G5B5A1_UNORM_PACK16_3D_MIP",
            "R5G6B5_UNORM_PACK16_3D_MIP",
            "R8G8B8A8_UNORM_3D_MIP",
            "R8G8B8_UNORM_3D_MIP",
            "R8G8_UNORM_3D_MIP",
            "R8_SINT_3D_MIP",
            "R8_SRGB_3D_MIP",
            "R8_UINT_3D_MIP",
            "R8_UNORM_3D_MIP"
        ]
    }
}
