{
    "description": "Test extraction from ZLIB compressed texture.",
    "command": "ktx extract --testrun --raw input/ktx2/valid_${subcase}_2D_ZLIB${level}.ktx2 output/extract/raw_zlib/output_${subcase}_2D_ZLIB${level}.raw",
    "status": 0,
    "outputs": {
        "output/extract/raw_zlib/output_${subcase}_2D_ZLIB${level}.raw": "golden/extract/raw_zlib/output_${subcase}_2D_ZLIB${level}.raw"
    },
    "args": {
        "level": [
            "_1",
            "_9"
        ],
        "subcase": [
            "R8_UNORM",
            "R8G8_UNORM",
            "R8G8B8_UNORM",
            "R8G8B8A8_UNORM",
            "ASTC_4x4_UNORM_BLOCK",
            "R4G4_UNORM_PACK8",
            "R5G6B5_UNORM_PACK16",
            "R4G4B4A4_UNORM_PACK16",
            "A2B10G10R10_UNORM_PACK32",
            "R16G16B16A16_UINT",
            "A2R10G10B10_UINT_PACK32",
            "R32G32B32A32_SFLOAT",
            "B10G11R11_UFLOAT_PACK32",
            "X8_D24_UNORM_PACK32",
            "S8_UINT",
            "D32_SFLOAT_S8_UINT"
        ]
    }
}
